            COVERAGE_TOTAL: ${{ needs.package_coverage.outputs.coverage_total }}
            PACKAGE_NAME: ${{ needs.python_package.outputs.package_name }}
            PACKAGE_VERSION: ${{ needs.python_package.outputs.package_version }}
